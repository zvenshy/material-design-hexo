<header id="header">
  <div id="banner"></div>
  <div id="header-outer" class="outer">
    <div id="header-title" class="inner">
      <h1 id="logo-wrap">
        <a href="<%- url_for() %>" id="logo"><%= config.title %></a>
      </h1>
      <% if (theme.subtitle){ %>
        <h2 id="subtitle-wrap">
          <a href="<%- url_for() %>" id="subtitle"><%= theme.subtitle %></a>
        </h2>
      <% } %>
    </div>
    <div id="header-inner" class="inner navbar navbar-default">
      <nav id="main-nav" class="navbar-collapse collapse navbar-responsive-collapse">
        <a id="main-nav-toggle" class="nav-icon"></a>
        <ul class="nav navbar-nav">
          <% for (var i in theme.menu){ %>
            <li><a class="main-nav-link btn btn-flat btn-primary" href="<%- url_for(theme.menu[i]) %>"><%= i %></a></li>
          <% } %>
        </ul>

      </nav>
      <nav id="sub-nav">

        <form action="//google.com/search" method="get" accept-charset="UTF-8" class="navbar-form navbar-left">
          <input type="text" name="q" results="0" class="form-control col-lg-8 empty" placeholder="Search">
        </form>
      </nav>
      <div id="search-form-wrap">
        <%- search_form({button: '&#xF002;'}) %>
      </div>
    </div>
  </div>
</header>